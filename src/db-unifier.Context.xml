<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-lazy-init="true">

	<bean id="dbSource1" class="org.sosostudio.dbunifier.dbsource.JdbcDbSource">
		<constructor-arg value="com.mysql.jdbc.Driver" />
		<constructor-arg value="jdbc:mysql://localhost:3306/test" />
		<constructor-arg value="root" />
		<constructor-arg value="55780029" />
	</bean>

	<bean id="dbSource2" class="org.sosostudio.dbunifier.dbsource.JdbcDbSource">
		<constructor-arg value="com.mysql.jdbc.Driver" />
		<constructor-arg value="jdbc:mysql://localhost:3306/test" />
	</bean>

	<bean id="dbSource3" class="org.sosostudio.dbunifier.dbsource.JndiDbSource">
		<constructor-arg value="java:test" />
	</bean>

	<bean id="dbSource4" class="org.sosostudio.dbunifier.dbsource.JndiDbSource">
		<constructor-arg value="java:test" />
		<constructor-arg value="root" />
		<constructor-arg value="55780029" />
	</bean>

</beans>